<style>
  .hidden {
    display: none;
    }

    main {
    }

    .visually-hidden {
        opacity: 0;
        position: fixed;
        top: 0; 
    }


    /*------------------------------------*\
        #Section Header
    \*------------------------------------*/

    #shopify-section-header {
        will-change: transform;
        width: 100%;
        top: 0;
        z-index: 10;
        top: 0;    position: fixed;
      }
      
      .shopify-section-header-sticky {
        position: fixed;
        top: 0;
        z-index: 10;
      }
      
      .shopify-section-header-hidden {
        /* transform: translateY(-100%); */
      }
      
      #shopify-section-header.animate {
        transition: transform 0.15s ease-out;
      }
      
      /* Main Header Layout */
      .header-wrapper {
        display: block;
        position: relative;
        background-color: rgb(var(--color-background));
      }
      
      .header-wrapper--border-bottom {
        border-bottom: 0.1rem solid rgba(var(--color-foreground), 0.2);
      }





    /*------------------------------------*\
        #Header
    \*------------------------------------*/

    sticky-header {
      z-index: 20; 
      top: 0; 
    }


    @media only screen and (min-width: 975px) {
      sticky-header {
        z-index: 2;
      }
    }

    .has-dropdown-open .header {
      padding-bottom: 34rem;
      background: linear-gradient(to top, transparent 0%, #000000e5 100%, #000000 90%);
      transition:  all .6s cubic-bezier(0.165, 0.84, 0.44, 1);
      transition-delay: 0s;
    }

    .header {
      display: grid;
      grid-template-areas: 'heading  icons left-icon';
      grid-template-columns: 2fr  1fr;
      align-items: center;
      padding-top: .5rem;
      padding-bottom: 1rem;
      padding-bottom: 1rem;
      height: 8rem;
      padding-left: .5rem;
      padding-right: 1rem;
      transition:  all .6s cubic-bezier(0.165, 0.84, 0.44, 1);
    }

    .header:hover:after {
      height: 12rem;

    }

    .header:after {
      content: " "; 
      position: absolute;
      top: 0; 
      left: 0; 
      right: 0; 
      width: 100%;
      height: 10rem;
      z-index: -1;
      transition: all var(--duration-default) ease 0s;
      background: linear-gradient(to top, transparent 0%, rgba(0, 0, 0, 0.898) 95%, rgb(0, 0, 0) 95%);
    }


    .header.is-product-page:after {
      opacity: 0;
    }


    .header.is-product-page .header__logo {
      fill: black;
      color: black;
    }


    .header.menu-is-open .header__logo {
      fill: #fff;
      color: #fff;
    }

    @media screen and (min-width: 990px) {
      .header {
        padding-top: 1rem;
        padding-bottom: 11rem;
        padding-left: 3rem;
        padding-right: 3rem;
      }


    .header.is-product-page.menu-is-open .header__logo {
      fill: black;
      color: black;
    }

      .header.is-product-page:hover, 
      .header.is-product-page:focus {
        opacity: 1;
      }

      .header--has-menu:not(.header--middle-left) {
        padding-bottom: 0;
      }

      .header--top-left {
        grid-template-areas:
          'heading icons'
          'navigation navigation';
        grid-template-columns: 1fr auto;
      }

      .header--middle-left {
        grid-template-areas: 'heading navigation icons';
        grid-template-columns: auto 1fr auto;
        column-gap: 2rem;
      }

      .header--top-center {
        grid-template-areas:
          'left-icon heading icons'
          'navigation navigation navigation';
      }

      .header--middle-left .header__inline-menu {
        min-width: 45rem;
      }

      .header:not(.header--middle-left) .header__inline-menu {
        margin-top: 1.05rem;
      }
    }





    .header__logo {
        width: 1.5rem;
        height: 4.25rem;
        color: #fff;
        fill: #fff;
        filter: drop-shadow(2px 4px 10px rgba(0,0,0, .1));
    }

    @media screen and (min-width: 990px) {
      .header__logo {
        width: 2rem;
        height: 4.5rem;
      }

    }


    @media screen and (min-width: 1100px) {
      .header__logo {
        width: 2rem;
        height: 4.75rem;
      }

    }



    .header *[tabindex='-1']:focus {
      outline: none;
    }

    .header__heading {
      margin: 0;
      line-height: 0;
    }

    .header > .header__heading-link {
      line-height: 0;
    }

    .header__heading,
    .header__heading-link {
      grid-area: heading;
      justify-self: flex-start;
    }

    .header__heading-link {
      display: inline-block;
      padding: 0.75rem;
      text-decoration: none;
      word-break: break-word;
    }

    .header__heading-link:hover .h2 {
      color: rgb(var(--color-foreground));
    }

    .header__heading-link .h2 {
      line-height: 1;
      color: rgba(var(--color-foreground), 0.75);
    }

    .header__heading-logo {
      height: auto;
      width: 100%;
    }

    @media screen and (min-width: 990px) {
      .header__heading-link {
        margin-left: -0.75rem;
      }

      .header__heading,
      .header__heading-link {
        justify-self: start;
      }

      .header--top-center .header__heading-link,
      .header--top-center .header__heading {
        justify-self: center;
      }
    }


    header-drawer {
      justify-self: flex-end;
      z-index: 0;
    }

    @media screen and (min-width: 990px) {
      header-drawer {
        display: none;
      }
    }







    /*------------------------------------*\
        #Header Icons
    \*------------------------------------*/

    .header__icons {
      display: flex;
      grid-area: icons;
      justify-self: end;
      position: relative;
      top: -1.1rem;
    }


    @media screen and (min-width: 975px) {
      .header__icons {
        top: 0; 
      }
    }




    .header__icon:not(.header__icon--summary),
    .header__icon span {
      display: flex;
      align-items: center;
      justify-content: center;
    }

    .header__icon span {
      height: 100%;
    }

    .header__icon::after {
      content: none;
    }

    .header__icon:hover {
      cursor: pointer;
    }


    .header__icon:hover .icon,
    .modal__close-button:hover .icon {

    }

    .header__icon .icon {
      height: 2rem;
      width: 2rem;
      fill: none;
      vertical-align: middle;
    }

    .header__icon,
    .header__icon--cart .icon {
      width: 2.5rem;
    }


    @media screen and (min-width: 975px) {
      .header__icon,
      .header__icon--cart .icon {
        width: 3rem;
      }
    }


    .header__icon--cart {
      position: relative;
      font-size: .85rem;
      margin-right: 1rem;
    }


    @media screen and (min-width: 975px) {
      .header__icon--cart {
        font-size: var(--h6);
      }
    }

    .cart-count-bubble {
      position: absolute;
      bottom: .45rem;
    }


    @media screen and (min-width: 975px) {
      .cart-count-bubble {
        bottom: .35rem;
      }
    }

      
      /* @media screen and (max-width: 989px) {
        menu-drawer ~ .header__icons .header__icon--account {
          display: none;
        }
      }

    .header__icon {
      list-style-type: none;
      }

    */



    /*------------------------------------*\
        #Header Menu Icon
    \*------------------------------------*/


    .header__icon--menu {
      border-radius: 100%; 
      border: 1px solid white; 
      background: transparent;
      width: 2.15rem; 
      height: 2.15rem;
      position: relative;
      top: -1rem;
    }



    .header.is-product-page .header__icon--menu {
      border-color: black;
    }

    .header__icon--menu.is-close {
      border: 1px solid white  !important;  
    }

    .header__icon--menu.is-active {

    }


    .header.is-product-page .header__icon--menu   .header__icon__inner:after,
    .header.is-product-page .header__icon--menu   .header__icon__inner:before {
      background-color: black;
    }


    .header__icon--menu .header__icon__inner {
      display: block;
      position: absolute;
      opacity: 1;
      width: 45%;
      transform: scale(1);
      transition: transform 150ms ease, opacity 150ms ease;
    }


    .header__icon--menu  .header__icon__inner:after,
    .header__icon--menu  .header__icon__inner:before {
      content: " "; 
      height: 60%;  
      width: 1px; 
      display: block;
      position: absolute;
      top: 0; 
      bottom: 0; 
      right: 0; 
      left: 0; 
      background-color: #fff;
      margin: auto;
      transition: all var(--duration-default) ease;
    }

    .header__icon--menu  .header__icon__inner:after {
      transform: rotate(
        90deg);
        top: -.5rem;
    }

    .header__icon--menu  .header__icon__inner:before {
      transform: rotate(-90deg);
      top: .5rem;
    }


    .header__icon--menu.is-close  .header__icon__inner:after {
      transform: rotate(
        45deg);
        top: 0; 
        background-color: white !important;
    }

    .header__icon--menu.is-close  .header__icon__inner:before {
      transform: rotate(-45deg);
      top: 0; 
      background-color: white !important;
    }



